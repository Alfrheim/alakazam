{
  "name": "pixijs-webpack-setup",
  "version": "1.0.0",
  "description": "A boilerplate for pixijs with webpack",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --mode=development",
    "test": "jest --config jest.config.js --coverage",
    "test:watch": "jest --config jest.config.js --watchAll",
    "build": "webpack --config webpack.config.js --mode=production"
  },
  "keywords": [
    "pixjs",
    "webpack"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.14.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "transform-loader": "^0.2.4",
    "brfs": "^2.0.2"
  },
  "author": "Daniel Jordi Perez <danipq26@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "pixi.js": "^4.8.6"
  }
}
